{
  "name": "@dangerusslee/gitlab-mcp-server",
  "version": "0.2.12",
  "description": "GitLab MCP Server - A Model Context Protocol server for GitLab integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/src/*.js",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "prepare": "npm run build",
    "test": "vitest run"
  },
  "bin": {
    "gitlab-mcp-server": "./dist/index.js"
  },
  "keywords": [
    "gitlab",
    "mcp",
    "model-context-protocol",
    "llm",
    "ai",
    "integration"
  ],
  "author": "Ion (Nalyk) Calmis",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.12",
    "axios-mock-adapter": "^2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangerusslee/mcp-gitlab-server.git"
  },
  "bugs": {
    "url": "https://github.com/dangerusslee/mcp-gitlab-server/issues"
  },
  "homepage": "https://github.com/dangerusslee/mcp-gitlab-server#readme",
  "publishConfig": {
    "access": "public"
  }
}
